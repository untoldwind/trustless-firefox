{
  "defaultSeverity": "error",
  "jsRules": {},
  "rules": {
    "no-floating-promises": [true, "AxiosPromise"],
    "trailing-comma": [
      true,
      {
        "multiline": {
          "objects": "always",
          "arrays": "always",
          "functions": "never",
          "typeLiterals": "ignore"
        },
        "esSpecCompliant": true
      }
    ]
  },
  "rulesDirectory": []
}